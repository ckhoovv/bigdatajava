function changeFontSize ()
{
    if ( fontSize == 0 )
    {
        alert ('최소 폰트 사이즈입니다.') ;
        fontSize = 1 ;
        return false;
    }
    else if ( fontSize == 5 )
    {
        alert ('최대 폰트 사이즈입니다.') ;
        fontSize = 4 ;
        return false;
    }

    if ( fontSize == 1 )
    {
        $('.article_txt').css({'font-size':'16px'});
    }
    else if ( fontSize == 3 )
    {
        $('.article_txt').css({'font-size':'20px'});
    }
    else if ( fontSize == 4 )
    {
        $('.article_txt').css({'font-size':'22px'});
    }
    else
    {
        $('.article_txt').css({'font-size':'18px'});
    }

    callCSset('pfs', fontSize) ;
    return false;
}
</script>
